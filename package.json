    "url": "https://github.com/travist/resourcejs/issues"
  },
  "homepage": "https://github.com/travist/resourcejs",
  "dependencies": {
    "@koa/router": "^9.3.1",
    "koa-qs": "^3.0.0",
    "debug": "^4.1.1",
    "fast-json-patch": "^3.0.0-1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "moment": "^2.28.0",
